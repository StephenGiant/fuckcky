<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="topexammodel"
            type="com.nsk.app.bussiness.healthy.viewmodel.TopExamViewModel"/>
    </data>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<!--高端体检-->
    <!--android:text="@string/phy_pro_titledes"-->
    <TextView
        android:layout_marginTop="@dimen/space_15"
        android:layout_marginLeft="@dimen/space_15"
        android:layout_marginRight="@dimen/space_15"
        android:paddingBottom="@dimen/space_15"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/tv_phyprodes"
        android:textColor="@color/title_black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:background="#88efefef"
        android:paddingTop="@dimen/space_12"
        android:paddingBottom="@dimen/space_12"
        android:paddingLeft="@dimen/space_20"
        android:paddingRight="@dimen/space_20"
        app:layout_constraintTop_toBottomOf="@id/tv_phyprodes"
        android:id="@+id/tv_phyprodetail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/background"
        app:layout_constraintTop_toBottomOf="@id/tv_phyprodetail"
        />
    <Button
        android:id="@+id/btn_advisory"
        android:background="@color/health_green"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="立即咨询"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</android.support.constraint.ConstraintLayout>
</layout>